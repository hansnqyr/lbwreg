<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"/>
    <meta http-equiv="Content-Style-Type" content="text/css"/>

    <title>LBW {{ year }} in {{ location }}</title>

    <script language="javascript1.1" src="/js/hide.js" type="text/javascript"></script>
    <script language="javascript1.1" src="/js/updated.js" type="text/javascript"></script>
    <script src="/js/utils.js" type="text/javascript"></script>
    <link rel="shortcut icon" href="/images/wandertux.ico"/>
    <link rel="stylesheet" href="/reg.css" type="text/css"/>
    {% block maps_js %}
    {% endblock %}
</head>
<body class='protected'>

<div class='content'>
    <div class='logo'>
        <a href="/welcome.php">
            <img
                    src="../images/hiking_tux-116x149.jpg"
                    alt="Hiking Tux"
                    border="0"
                    width="116"
                    height="149"
                    />
        </a>
    </div>
    <div class='header'>
        <a href="{{ eventhost }}">
            <span style='font-size: 36px;'>LBW {{ year }}</span>
        </a>

        <h1>{{ location }}<br/>
            {{ start_date }} - {{ end_date }}</h1>
        <br/>
        LBW {{ year }} welcomes {{ logged_in_name }}
        {% include 'navbar.twig' %}
        {% if admin %}
            {% include 'adminbar.twig' %}
        {% endif %}
    </div>
    <div class="body">
        {% block body %}
            Someone messed everything up here!
        {% endblock %}
    </div>


    {# include 'footer.twig' #}
</div>
</body>
</html>